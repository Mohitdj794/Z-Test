create table userLogin (id int PRIMARY KEY AUTO_INCREMENT, userName varchar(50) UNIQUE, email varchar(50) UNIQUE, password varchar(100) NOT NULL);

create table Test_Title (Test_id int PRIMARY KEY AUTO_INCREMENT, TestTitle varchar(100) UNIQUE, TestDuration int );	
           INSERT->insert into Test_Title(TestTitle, TestDuration)Values(Amazon Cloud Services,14);
           
 create table Test_Question (Question_id int PRIMARY KEY AUTO_INCREMENT, Question LONGTEXT, Test_id int);
 ALTER TABLE Test_Question add constraint FK_Test_id FOREIGN KEY (Test_id) references Test_Title(Test_id);
            INSERT-> insert into Test_Question(Question,Test_id)Values('What is Aws,1);
 
 create table Test_Result(Answer_id int PRIMARY KEY AUTO_INCREMENT,Options LONGTEXT,Answer varchar(100),Question_id int);
  ALTER TABLE Test_Result add constraint FK_Question_id FOREIGN KEY (Question_id) references Test_Question(Question_id);
