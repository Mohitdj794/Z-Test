join 2 tables Test_id,Test_Question.Test_id

Select Test_Title.TestTitle,Test_Question.Question FROM Test_Title INNER JOIN Test_Question ON Test_Title.Test_id=Test_Question.Test_id;


selecting 3 tables;
select Test_Title.TestTitle,Test_Question.Question,Test_Option.option1,Test_Option.option2,Test_Option.option3,Test_Option.option4 From Test_Title,Test_Question,Test_Option Where Test_Title.Test_id=Test_Question.Test_id AND Test_Question.Question_id=Test_Option.Question_id;

select Test_Title.TestTitle,Test_Question.Question,Test_Option.option1,Test_Option.option2,Test_Option.option3,Test_Option.option4 From Test_Title INNER JOIN Test_Question ON Test_Title.Test_id=Test_Question.Test_id INNER JOIN Test_Option ON Test_Question.Question_id=Test_Option.Question_id;

select Test_Title.TestTitle,Test_Question.Question,Test_Option.option1,Test_Option.option2,Test_Option.option3,Test_Option.option4 From Test_Title INNER JOIN Test_Question ON Test_Title.Test_id=Test_Question.Test_id INNER JOIN Test_Option ON Test_Question.Question_id=Test_Option.Question_id where Test_Title.Test_id=6;


join  4  tables

select Test_Title.TestTitle,Test_Question.Question,Test_Option.option1,Test_Option.option2,Test_Option.option3,Test_Option.option4,Test_Answer.Answer From Test_Title,Test_Question,Test_Option,Test_Answer Where Test_Title.Test_id=Test_Question.Test_id AND Test_Question.Question_id=Test_Option.Question_id AND Test_Question.Question_id=Test_Answer.Question_id;

select Test_Title.TestTitle,Test_Question.Question,Test_Option.option1,Test_Option.option2,Test_Option.option3,Test_Option.option4,Test_Answer.Answer From Test_Title INNER JOIN Test_Question ON Test_Title.Test_id=Test_Question.Test_id INNER JOIN Test_Option ON Test_Question.Question_id=Test_Option.Question_id INNER JOIN Test_Answer ON Test_Question.Question_id=Test_Answer.Question_id where Test_Title.Test_id=6;



final data



Select Test_Title.TestTitle,Test_Title.TestDuration,Test_Question.Question_id,Test_Question.Question,Test_Result.Options,Test_Result.Answer FROM Test_Title INNER JOIN Test_Question ON Test_Title.Test_id=Test_Question.Test_id INNER JOIN Test_Result ON Test_Question.Question_id=Test_Result.Question_id where Test_Title.Test_id=6;






select Test_Question.Question,Test_Result.Options,Test_Result.Answer from Test_Question INNER JOIN Test_Result ON Test_Question.Question_id=Test_Result.Question_id where Test_Question.Question_id=74;






